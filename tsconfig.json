{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": false,
        // "sourceMap": true,
        "alwaysStrict": true,
        "allowUnreachableCode": false,
        "noUnusedLocals": true,
        "jsx": "react",
        "noUnusedParameters": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "skipLibCheck": true,
        "experimentalDecorators": true
    },
    "dependencies": {}
}